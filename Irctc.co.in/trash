@Test
	public void screenShot()
	{
		railLogin.loginToApplication();
		TakeErrorScreenShot("qedge");
	}
	
	String screenshotfilepath;
	public String TakeErrorScreenShot(String fname)
	{
		File scrFile = ((TakesScreenshot)driver).getScreenshotAs(OutputType.FILE);
		screenshotfilepath = "C:\\Java\\Selenium\\Irctc.co.in\\ScreenShots\\"+fname+".png";
        try {
			FileUtils.copyFile(scrFile, new File(screenshotfilepath));
        	//FileUtils.copyFile();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
        scrFile = null;
        System.out.println(screenshotfilepath);
        return screenshotfilepath;
	}

	
	public void smokeSuite() {
		// TODO Auto-generated method stub
		
		smoke.appendChild(child1);
		smoke.appendChild(child2);
		System.out.println("Test Suite : Smoke");
		flag = tc.registerToIrctc();
		if(flag)
		{
			child1.log(LogStatus.PASS, "Register to Irctc", "Register to Irctc");
		}
		else
		{
			child1.log(LogStatus.FAIL, "Failed to Register",child1.addScreenCapture(TakeErrorScreenShot("registerToIrctc")));
		}
		
		tc.userLogin();
		tc.accountActions();
		tc.searchTrain();
		tc.searchAndBookTrain();
		tc.pnrStatusCheck();

		
		
		flag = tc.searchTrain();
		if(flag)
		{
			child2.log(LogStatus.PASS, "FlightAndHotel", "Vacation Planned with Filght and Hotel");
		}
		else
		{
			child2.log(LogStatus.FAIL, "Failed to Book Filght and Hotel",child1.addScreenCapture(TakeErrorScreenShot("FlightHotel")));
		}
		mySmoke.log(LogStatus.INFO, "Execution Done");
		myExtent.flush();
		myExtent.endTest(mySmoke);
	}
	
